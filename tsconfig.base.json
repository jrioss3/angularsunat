{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@rentas/juridico": ["libs/juridico/src/index.ts"],
      "@rentas/shared/assets": ["libs/shared/assets/src/index.ts"],
      "@rentas/shared/environments": ["libs/shared/environments/src/index.ts"],
      "@rentas/natural": ["libs/natural/src/index.ts"],
      "@path/natural/*": ["libs/natural/src/lib/*"],
      "@path/juridico/*": ["libs/juridico/src/lib/*"],
      "@rentas/cabecera": ["libs/cabecera/src/index.ts"],
      "@rentas/menu": ["libs/menu/src/index.ts"],
      "@rentas/shared/constantes": ["libs/shared/constantes/src/index.ts"],
      "@rentas/casillas": ["libs/casillas/src/index.ts"],
      "@rentas/shared/types": ["libs/shared/types/src/index.ts"],
      "@path/casillas/*": ["libs/casillas/src/lib/*"],
      "@rentas/shared/core": ["libs/shared/core/src/index.ts"],
      "@rentas/lista-errores": ["libs/lista-errores/src/index.ts"],
      "@rentas/constancia": ["libs/constancia/src/index.ts"],
      "@rentas/shared/utils": ["libs/shared/utils/src/index.ts"],
      "@rentas/consultas": ["libs/consultas/src/index.ts"],
      "@rentas/pasarela": ["libs/pasarela/src/index.ts"],
      "@rentas/shared/iu": ["libs/shared/iu/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
